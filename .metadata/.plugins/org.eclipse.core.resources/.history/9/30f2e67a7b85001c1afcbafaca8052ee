<%
	String taibay = request.getParameter("city");
	String newyork = request.getParameter("city");
	String london = request.getParameter("city");
	String paris = request.getParameter("city");
	
	if(taibay.equals("taibay")){
		response.sendRedirect("travel_taibay.jsp");
	}else if(newyork.equals("newyork")){
		response.sendRedirect("travel_newyork.jsp");
	}else if(london.equals("london")){
		response.sendRedirect("travel_london.jsp");
	}else if(paris.equals("paris")){
		response.sendRedirect("travel_paris.jsp");
	}
	
%>
</body>
</html>